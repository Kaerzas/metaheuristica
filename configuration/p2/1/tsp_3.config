		<solution>problems.tsp.SolutionTSP</solution>
		<solGenerator name="problems.tsp.SolGeneratorTSP"></solGenerator>
		<instance name="problems.tsp.InstanceTSP">
			<data>examples/TSPInstances/berlin52.tsp</data>
